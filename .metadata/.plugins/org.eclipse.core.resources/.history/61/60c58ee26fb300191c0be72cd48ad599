package com.start.java;

public class Armstrong13 {
	public static void main(String[] args) {
		int i;
		int number=153;
		int temp=number;
		
		int rem=0;
		while(number!=0) {
			rem=number%10;
			temp=temp+rem*rem*rem;
			number=number/10;
			
			
		}
		if(temp==number) {
			System.out.println("it is Armstrong number");
		}
		else{
			System.out.println("it is not Armstrong number");
	
		}
	}

}
