package com.start.java;

public class Armstrong {
	public static void main(String[] args) {
		int number=153;
		int armos=number;
		int rem=0;
	    int temp=0;
		while(number!=0) {
			rem=rem%10;
			number=number/10;
			temp=temp+rem*rem*rem;
			
			
		}
		if(temp==armos) {
			System.out.println("it is armstrong");
			
				
			}
		else {
			System.out.println("it is not armstrong");
		}
			}
			
		}
	
	


